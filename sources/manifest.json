{
    "manifest_version": 2,
    "name": "Disrupt to Bullshit",
    "version": "1.0",
    "description": "Replaces various versions of 'disrupt' with 'bullshit'.",
    "content_scripts":
    [
        {
            "matches": ["*://*/*"],
            "match_about_blank": true,
            "js": [
                "disrupt_to_bullshit.js",
                "walk_text_nodes.js",
                "create_verb_converter.js",
                "main.js"
            ],
            "all_frames": true
        }
    ]
}
